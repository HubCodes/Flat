%{
%}

%start program

%token <int> INT
%token EOF
%token PLUS MINUS

%left PLUS MINUS

%type <Expr option> program

%%

program:
    | expr      { Some $1 }
    | EOF       { None }

expr:
    | expr PLUS expr      { Add ($1, $3) }
    | expr MINUS expr     { Sub ($1, $3 ) }
    | INT                 { Literal (Int $1) }
